	<queryString>
		<![CDATA[SELECT BHR_EMP_JOB.JOB_CD,
  BTHR_JOB_CD.JOB_CD_DESCR as title,
  BHR_EMP_JOB.NBR_ANNUAL_PYMTS as annual_payments,
  BHR_EMP_JOB.REG_HRS_WRK as regular_hours,
  BHR_EMP_JOB.NBR_REMAIN_PYMTS as remain_payments,
  BHR_EMP_JOB.CONTR_AMT as annual_salary,
  BHR_EMP_JOB.DLY_RATE_OF_PAY as daily_rate,
  BHR_EMP_JOB.PAY_RATE as pay_rate,
  BHR_EMP_JOB.OVTM_RATE as overtime_rate
 FROM BHR_EMP_JOB,
     BTHR_JOB_CD
 WHERE BHR_EMP_JOB.EMP_NBR = $P{as_empnbr}
  AND BHR_EMP_JOB.PAY_FREQ = $P{as_payfreq}
  AND BHR_EMP_JOB.CYR_NYR_FLG = 'C'
  AND BTHR_JOB_CD.CYR_NYR_FLG = 'C'
  AND BHR_EMP_JOB.JOB_CD = BTHR_JOB_CD.JOB_CD
ORDER BY BTHR_JOB_CD.JOB_CD_DESCR]]>
	</queryString>

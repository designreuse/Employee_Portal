       BHR_EMP_EXTRA_DUTY.EXTRA_DUTY_AMT as amount,
       BHR_EMP_EXTRA_DUTY.REMAIN_AMT as remain_amount,
       BHR_EMP_EXTRA_DUTY.REMAIN_PYMTS as remain_payments
  FROM BHR_EMP_EXTRA_DUTY,
       BTHR_EXTRA_DUTY
 WHERE BHR_EMP_EXTRA_DUTY.EMP_NBR = $P{as_empnbr}
   AND BHR_EMP_EXTRA_DUTY.PAY_FREQ = $P{as_payfreq}
